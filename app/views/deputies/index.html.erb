<div class="page-header">
  <h1>
    Deputies
    <a href="/deputies/new" class="btn btn-lg btn-success">New Deputy</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Party</th>
        <th>Sex</th>
        <th>State</th>
        <th>Telephone</th>
        <th>Email</th>
        <th>Picture</th>
        <th>Expenses</th>
        <th>Actions</th>
      </tr>

      <% @deputies.each do |deputy| %>
      <tr>
        <td><%= deputy.name %></td>
        <td><%= deputy.party %></td>
        <td><%= deputy.sex %></td>
        <td><%= deputy.state %></td>
        <td><%= deputy.telephone %></td>
        <td><%= deputy.email %></td>
        <td><%= deputy.picture %></td>
        <td><%= deputy.expenses %></td>
        <td>
          <a href="/deputies/<%= deputy.id %>" class="btn btn-primary">Show</a>
          <a href="/deputies/<%= deputy.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_deputy/<%= deputy.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
