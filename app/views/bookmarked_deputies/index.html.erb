<div class="page-header">
  <h1>
    Bookmarked deputies
    <a href="/bookmarked_deputies/new" class="btn btn-lg btn-success">New Bookmarked deputy</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Deputy</th>
        <th>Actions</th>
      </tr>

      <% @bookmarked_deputies.each do |bookmarked_deputy| %>
      <tr>
        <td><%= bookmarked_deputy.user_id %></td>
        <td><%= bookmarked_deputy.deputy_id %></td>
        <td>
          <a href="/bookmarked_deputies/<%= bookmarked_deputy.id %>" class="btn btn-primary">Show</a>
          <a href="/bookmarked_deputies/<%= bookmarked_deputy.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_bookmarked_deputy/<%= bookmarked_deputy.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
