<div class="page-header">
  <h3>Senator #<%= @senator.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @senator.name %></dd>

      <dt>Party</dt>
      <dd><%= @senator.party %></dd>

      <dt>Sex</dt>
      <dd><%= @senator.sex %></dd>

      <dt>State</dt>
      <dd><%= @senator.state %></dd>

      <dt>Telephone</dt>
      <dd><%= @senator.telephone %></dd>

      <dt>Email</dt>
      <dd><%= @senator.email %></dd>

      <dt>Picture</dt>
      <dd><%= @senator.picture %></dd>

      <dt>Expenses</dt>
      <dd><%= @senator.expenses %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/senators" class="btn btn-primary">
        Back
      </a>
      <a href="/senators/<%= @senator.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_senator/<%= @senator.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
