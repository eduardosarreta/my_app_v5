<div class="page-header">
  <h1>
    Bookmarked senators
    <a href="/bookmarked_senators/new" class="btn btn-lg btn-success">New Bookmarked senator</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Senator</th>
        <th>Actions</th>
      </tr>

      <% @bookmarked_senators.each do |bookmarked_senator| %>
      <tr>
        <td><%= bookmarked_senator.user_id %></td>
        <td><%= bookmarked_senator.senator_id %></td>
        <td>
          <a href="/bookmarked_senators/<%= bookmarked_senator.id %>" class="btn btn-primary">Show</a>
          <a href="/bookmarked_senators/<%= bookmarked_senator.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_bookmarked_senator/<%= bookmarked_senator.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
